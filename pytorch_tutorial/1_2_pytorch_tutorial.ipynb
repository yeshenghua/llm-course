    "import torch\n",
    "\n",
    "x = torch.rand(2,2)\n",
    "y = torch.rand(2,2)\n",
    "\n",
    "# element-wise operations\n",
    "z = torch.add(x,y)\n",
    "z = torch.mul(x,y)\n",
    "\n",
    "# y = y + x element-wise\n",
    "y.add_(x)\n",
    "\n",
    "print(x)\n",
    "print(y)\n",
    "print(z)\n",
    "\n",
    "# view\n",
    "x = torch.rand(4,4)\n",
    "print(x)\n",
    "y = x.view(16)\n",
    "print(y)\n",
    "y = x.view(-1, 8)\n",
    "print(y.size())\n"
